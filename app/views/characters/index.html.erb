<h1>Characters#index</h1>
<p>Find me in app/views/characters/index.html.erb</p>

<table>
    <tr>
        <td>Name</td>
        <td>Age</td>
        <td>Gender</td>
        <td>Voice Acter</td>
    </tr>
    <% @characters.each do |f| %>
    <tr>
        <td><%= f.name %></td>
        <td><%= f.age %></td>
        <td><%= f.gender %></td>
        <td><%= f.voice_acter %></td>
    </tr>
    <% end %>
</table>

    <%= form_with model: @character, url: "characters" do |f| %>
        <%= f.text_field :name %><br>
        <%= f.text_field :age %><br>
        <%= f.text_field :gender %><br>
        <%= f.text_field :voice_acter %><br>
        <%= f.submit "送信" %>
    <% end %>



